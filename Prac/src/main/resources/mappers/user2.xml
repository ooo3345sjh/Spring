<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user2">
	<insert id="insertUser2">
		INSERT INTO `user2` VALUES (#{uid}, #{name}, #{hp}, #{age});
	</insert>
	<select id="selectUser2" resultType="kr.co.prac.vo.User2VO">
		SELECT * FROM `user2` WHERE `uid`=#{uid}
	</select>
	<select id="selectUser2s" resultType="kr.co.prac.vo.User2VO">
		SELECT * FROM `user2`;
	</select>
	<delete id="updateUser2">
		UPDATE `user2` SET
			`name`=#{name},
			`hp`=#{hp},
			`age`=#{age}
		WHERE
			`uid`=#{uid};
	</delete>
	<update id="deleteUser2">
		DELETE FROM `user2` WHERE `uid`=#{uid};
	</update>
</mapper>